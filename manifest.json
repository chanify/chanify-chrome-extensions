{
    "name": "Chanify",
    "description": "Chanify is a safe and simple notification tools. For developers, system administrators, and everyone can push notifications to your iPhone.",
    "version": "0.9.0",
    "manifest_version": 3,
    "icons": {
        "16": "/images/app-16.png",
        "32": "/images/app-32.png",
        "48": "/images/app-48.png",
        "128": "/images/app-128.png"
    },
    "permissions": ["storage", "contextMenus"],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Chanify Client",
        "default_popup": "popup.html"
    },
    "options_page": "options.html"
}